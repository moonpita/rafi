  include ./catalogVars.pug
  include ../../widgets/header/header.pug
  
  
  block Variables

  include ../../shared/ui/inputs/checkboxInput.pug

  .container 
    h1.page-title.productsSubcategories__title Products
    form.catalog 
      .catalog-mob-top 
        .catalog-mob-top__filter
          span Categories <img src="../../shared/img/bottomArrowBlack.svg">
      .catalog-left.overlayContent
        .catalog-left-content 
          .acc-btn.catalog-left__block--btn.active
              span Categories 
              img(src="../../shared/img/arrowBottom.svg", alt="")
          .catalog-left__block--content.active
            a.catalog-left__block--link.back(href="./") 
              img(src="../../shared/img/redArrowBack.svg", alt="")
              span Back from category 
            a.catalog-left__block--link(href="./") category X
            a.catalog-left__block--link(href="./") category Z 
            a.catalog-left__block--link(href="./") category X
