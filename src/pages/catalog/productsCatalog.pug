            .catalog__item--title Air 
          a.catalog__item(href="https:wwww.google.com")
            img.catalog__item--img(src="./img/Rectangle 17.png", alt="")
            .catalog__item--title Airign
          a.catalog__item(href="https:wwww.google.com")
            img.catalog__item--img(src="./img/Rectangle 17-1.png", alt="")
            .catalog__item--title Air Conditioning and Mechanical Ventilation System design
          a.catalog__item(href="https:wwww.google.com")
            img.catalog__item--img(src="./img/Rectangle 17-2.png", alt="")
            .catalog__item--title Air Coal Venon System design
          a.catalog__item(href="https:wwww.google.com")
            img.catalog__item--img(src="./img/Rectangle 17.png", alt="")
            .catalog__item--title Airign
          a.catalog__item(href="https:wwww.google.com")
            img.catalog__item--img(src="./img/Rectangle 17-1.png", alt="")
            .catalog__item--title Air Conditioning and Mechanical Ventilation System design
        include ../../widgets/pagination/pagination.pug
        +pagination('catalog-pagination')


  include ../../widgets/footer/footer.pug
  script(src="../../shared/lib/nouislider.min.js" defer) 
  script(src="../../shared/lib/accordion.js" defer) 
  script(src="../../shared/lib/inputsController.js" defer) 
